[bar/nonk]
font-0 = "Hack:pixelsize=11:0"
separator = " | "
modules-left = bspwm
modules-center = title
modules-right = keyboard stardate date
tray-position = right

[module/bspwm]
type = internal/bspwm
enable-scroll = false

[module/title]
type = internal/xwindow

[module/keyboard]
type = internal/xkeyboard
blacklist-0 = num lock
blacklist-1 = scroll lock

[module/stardate]
type = custom/script
exec = stardate
interval = 1

[module/date]
type = internal/date
date = %d.%m.%Y
time = %H:%M:%S
label = %date% %time%
